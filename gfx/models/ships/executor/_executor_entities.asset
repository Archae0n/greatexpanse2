
entity = {
	name = "executor_entity"
	pdxmesh = "mammalian_01_titan_frame_mesh"
	scale = 4.0
	
	locator = { name = "part1"	position = { 0 0 0 } rotation = { 0 0 0 } }
	locator = { name = "part2"	position = { 0 0 0 } rotation = { 0 0 0 } }
